#A sphere going to a downside desk
DEF SoccerBall
Transform{
    translation 11.0 -1.5 4.0
    scale 2 2 2
    children[
        DEF SoccerBallTouch TouchSensor{}
        Shape{
            appearance Appearance{
                material Material{}
                texture ImageTexture {
		            url "SoccerBallTexture.jpg"
	            }
            }
            geometry Sphere {
                radius 0.2
            }
        }
    ]
}
DEF SoccerBallTimer TimeSensor {
	cycleInterval 8.0
	loop TRUE
	startTime 0.0
	stopTime 0.1
}
DEF SoccerBallPosition  PositionInterpolator{
  key[0, 0.3 , 0.375, 0.625, 0.7, 1.0]
  keyValue[ 11.0 -1.5 4.0, 4.0 -0.55 3.0 , 2.0 -0.55 3.0, 2.0 -0.55 3.0, 4.0 -0.55 3.0 , 11.0 -1.5 4.0 ]
}
ROUTE SoccerBallTouch.touchTime TO SoccerBallTimer.set_startTime
ROUTE SoccerBallTimer.fraction_changed TO SoccerBallPosition.set_fraction
ROUTE SoccerBallPosition.value_changed TO SoccerBall.set_translation

