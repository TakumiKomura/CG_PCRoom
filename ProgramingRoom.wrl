#monitor
DEF Monitor
Transform{
    translation -7.0 1.65 -9.5
    rotation 0 0 1 3.14
    scale 6.0 6.0 6.0
    children[
        DEF MonitorTouch TouchSensor{}    
        Transform{
            translation 0.0 -0.0 -0.03
            children[
                Shape{
                    appearance Appearance{
                        material Material{
                            diffuseColor 0.01 0.01 0.01
                            emissiveColor 0.01 0.01 0.01
                            transparency 0.0
                            specularColor 0.1 0.1 0.1
                            shininess 0.32
                            ambientIntensity 0.5
                        }
                    }
                    geometry Cylinder {
                        radius 0.05
                        height 0.01
                    }
                }
            ]
        }
        Transform{
            translation 0.0 0.1 -0.014
            rotation 1 0 0 -0.2
            children[
                Shape{
                    appearance Appearance{
                        material Material{
                            diffuseColor 0.01 0.01 0.01
                            emissiveColor 0.05 0.05 0.05
                            transparency 0.0
                            specularColor 0.1 0.1 0.1
                            shininess 0.32
                            ambientIntensity 0.5
                        }
                    }
                    geometry Box{
                        size 0.25 0.125 0.01
                    }
                }
            ]
        }
        Transform{
            translation 0.0 0.07 -0.03
            scale 1 1 0.8
            children[
                Shape{
                    appearance Appearance{
                        material Material{
                            diffuseColor 0.01 0.01 0.01
                            emissiveColor 0.01 0.01 0.01
                            transparency 0.0
                            specularColor 0.1 0.1 0.1
                            shininess 0.32
                            ambientIntensity 0.5
                        }
                    }
                    geometry Cylinder{
                        height 0.15
                        radius 0.01
                    }
                }
            ]
        }
        #display
        Transform{
            translation 0.0 0.0 -0.008
            children[
                Shape {
                    appearance Appearance {
                        material DEF MonitorFacade Material {
                            diffuseColor 0.2 0.2 0.2
                            transparency 0.4
                            shininess 0.32
                        }
                    }
                    geometry IndexedFaceSet {
                        coord Coordinate {
                            point [
                                -0.11 0.15 -0.01,#0
                                -0.11 0.05 0.01,#1
                                0.11 0.05 0.01,#2
                                0.11 0.15 -0.01,#3
                            ]
                        }
                        coordIndex [
                            0, 1, 2, 3, -1
                        ]
                    }
                }
            ]
        }
    ]
}
